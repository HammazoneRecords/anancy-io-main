# Staging environment variables
# Copy this file to .env.staging and update values

# Application
ENVIRONMENT=staging
LOG_LEVEL=debug
WORKERS=2

# Security
SECRET_KEY=staging-secret-key-change-this
FLASK_SECRET_KEY=staging-flask-secret-key-change-this

# API Keys (consider using different keys for staging)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/anancyio_staging

# Redis
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=

# External Services
SEARXNG_URL=
SENTRY_DSN=
